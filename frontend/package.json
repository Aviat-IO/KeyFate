    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.22",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.28.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.2",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
