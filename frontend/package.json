{
  "name": "keyfate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate-env": "node ../scripts/validate-env.js",
    "sync-policies": "tsx scripts/sync-policy-documents.ts local",
    "sync-policies:staging": "tsx scripts/sync-policy-documents.ts staging",
    "sync-policies:prod": "tsx scripts/sync-policy-documents.ts production",
    "dev": "npm run validate-env && next dev",
    "build": "next build",
    "build:local": "npm run validate-env && next build",
    "start": "npm run validate-env && next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:introspect": "drizzle-kit introspect",
    "db:up": "drizzle-kit up",
    "db:proxy-staging": "cloud-sql-proxy --port=54321 keyfate-dev:us-central1:keyfate-postgres-staging",
    "db:proxy-prod": "cloud-sql-proxy --port=54321 keyfate-dev:us-central1:keyfate-postgres-production"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.11.0",
