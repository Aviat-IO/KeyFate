    "drizzle-orm": "^0.36.4",
    "lucide-react": "^0.462.0",
    "next": "15.4.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.7",
    "ogl": "^1.0.11",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.65.0",
    "shamirs-secret-sharing": "^2.0.1",
    "stripe": "^17.7.0",
